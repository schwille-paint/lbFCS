#### Figure
figure.edgecolor:white
figure.facecolor: None
figure.subplot.bottom:0.2
figure.subplot.top:0.99
figure.subplot.left:0.2
figure.subplot.right:0.99

#### Axis
axes.facecolor:None
axes.edgecolor:black
axes.linewidth:1

#### Grid
grid.color: None
grid.alpha: 0

#### Markers, Lines
lines.linewidth:1
lines.markersize:6
errorbar.capsize:5
axes.prop_cycle: cycler('color', ['black','green','blue','red','magenta','yellow','gray'])

#### Font
xtick.labelsize:18
ytick.labelsize:18
figure.titlesize:18
legend.fontsize:18
axes.titlesize:18
axes.labelsize:18
axes.labelcolor:black
mathtext.default:regular

#### Ticks
xtick.color:black
ytick.color:black
ytick.minor.visible: False
xtick.minor.visible: False
xtick.direction: out
ytick.direction: out

#### Legend
legend.borderaxespad:0.1
legend.loc:upper right
legend.markerscale:1
legend.facecolor: None
legend.edgecolor: None

#### Figure saving
figure.dpi:100
savefig.dpi:200
savefig.transparent:True